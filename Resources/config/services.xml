<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    
    <parameters>
        <parameter key="cnerta_breadcrumb.twig.extension.class">Cnerta\BreadcrumbBundle\Twig\MenuExtension</parameter>
    </parameters>

    <services>
        <service id="cnerta_breadcrumb.twig.extension" class="%cnerta_breadcrumb.twig.extension.class%">
            <tag name="twig.extension" />
            <argument type="service" id="knp_menu.helper" />
        </service>
    </services>
    
</container>
